doctype html
html(lang='en')
  head
    title Swagger compare results
    style
      include style.css
  body
    div.container
      table
        thead
          tr
            th API
            th STATUS
            th TESTCASE
            th RESULT
            th DESCRIPTION
        tbody
          each obj in resultResp
            if obj.result === 'NG'
              tr.result-ng
                td #{obj.api}
                td #{obj.status}
                td #{obj.testCase}
                td #{obj.result}
                td
                  each el, index in obj.descriptions
                    if index === obj.descriptions.length - 1
                      p #{el}
                    else
                      p #{el}
                      br
            else
              tr
                td #{obj.api}
                td #{obj.status}
                td #{obj.testCase}
                td #{obj.result}
                td
                  each el in obj.descriptions
                    p #{el}
                    br